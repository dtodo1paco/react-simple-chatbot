    "jest": "^22.4.2",
    "jest-dom": "^1.11.0",
    "jsdom": "^11.6.2",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.0.4",
    "postcss-loader": "^2.1.6",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.0.0",
    "react-tabs": "^2.2.2",
    "react-test-renderer": "^16.2.0",
    "reactstrap": "^6.3.1",

{
  "name": "react-simple-chatbot",
  "version": "1.0.0",
  "description": "A simple chatbot client using a custom Auth nodejs app to protect token using JWT and encryption",
  "main": "index.js",
  "homepage": "https://dtodo1paco.github.io/react-simple-chatbot",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "webpack --config ./webpack.config.js --mode production",
    "dev": "webpack-dev-server --config ./webpack.config.js --mode development --inline",
    "start": "webpack-dev-server --config ./webpack.config.js --mode production --inline",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --verbose --no-cache --runInBand --config __tests__/config/jest.unit.config.js",
    "test:unit:watch": "npm run test:unit -- --watchAll",
    "test:integration": "jest --verbose --no-cache --runInBand --config __tests__/config/jest.integration.config.js",
    "test:integration:watch": "npm run test:integration -- --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
